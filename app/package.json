{
  "name": "gagweb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "browserify -d -t [ babelify --presets [ @babel/preset-env  @babel/preset-react ] ] index.js | exorcist  dist/bundle.map.js > dist/bundle.min.js",
